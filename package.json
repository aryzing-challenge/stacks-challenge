{
  "name": "stacks-challenge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^18.7.0",
    "yarn": "^1.22.19"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:check": "eslint --max-warnings 0 --ext .ts,.tsx src",
    "lint:fix": "eslint --max-warnings 0 --ext .ts,.tsx src --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "fix": "yarn lint:fix; yarn format:fix",
    "types:check": "tsc",
    "ci": "yarn lint:check && yarn format:check && yarn types:check"
  },
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@mantine/core": "5.0.0",
    "@mantine/form": "5.0.0",
    "@mantine/hooks": "5.0.0",
    "@stacks/blockchain-api-client": "4.1.0",
    "@tabler/core": "1.0.0-beta9",
    "@tanstack/react-query": "4.0.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "2.0.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "prettier": "2.7.1",
    "typescript": "4.6.4",
    "vite": "3.0.0"
  }
}
